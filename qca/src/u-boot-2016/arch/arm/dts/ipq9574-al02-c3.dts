/dts-v1/;
#include "ipq9574-al02-c2.dts"
/ {
	machid = <0x8050201>;
	config_name = "config@al02-c3";

	aliases {
		pci0 = "/pci@28000000";
	};

	pci0: pci@28000000 {
		status = "ok";
		perst_gpio = <23>;
		pci_gpio {
			pci_rst {
				gpio = <23>;
				func = <0>;
				pull = <GPIO_PULL_UP>;
				oe = <GPIO_OD_ENABLE>;
				od_en = <GPIO_OD_DISABLE>;
				drvstr = <GPIO_8MA>;
			};
		};
	};
};
