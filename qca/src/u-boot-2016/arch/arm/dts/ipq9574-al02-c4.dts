/dts-v1/;
#include "ipq9574-al02-c1.dts"
/ {
	machid = <0x8050301>;
	config_name = "config@al02-c4";

	aliases {
		pci1 = "/pci@10000000";
	};

	pci1: pci@10000000 {
		status = "ok";
		perst_gpio = <26>;
		pci_gpio {
			pci_rst {
				gpio = <26>;
				func = <0>;
				pull = <GPIO_PULL_DOWN>;
				oe = <GPIO_OD_ENABLE>;
				od_en = <GPIO_OD_DISABLE>;
				drvstr = <GPIO_8MA>;
			};
		};
	};
};
