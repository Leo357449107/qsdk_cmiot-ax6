/dts-v1/;
#include "zynq-7000.dtsi"

/ {
	model = "Zynq MicroZED Board";
	compatible = "xlnx,zynq-microzed", "xlnx,zynq-7000";

	aliases {
		serial0 = &uart1;
		spi0 = &qspi;
	};

	memory {
		device_type = "memory";
		reg = <0 0x40000000>;
	};
};

&qspi {
	status = "okay";
};

&uart1 {
	u-boot,dm-pre-reloc;
	status = "okay";
};
