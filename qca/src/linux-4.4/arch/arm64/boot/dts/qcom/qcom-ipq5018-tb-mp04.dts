#include "qcom-ipq5018-mp03.3.dts"

/ {
	#address-cells = <0x2>;
	#size-cells = <0x2>;
	model = "Qualcomm Technologies, Inc. IPQ5018/TB-MP04";
	compatible = "qcom,ipq5018-tb-mp04", "qcom,ipq5018";
	interrupt-parent = <&intc>;

	soc {
		lpass: lpass@0xA000000{
			status = "disabled";
		};

		pcm: pcm@0xA3C0000{
			status = "disabled";
		};

		pcm_lb: pcm_lb@0 {
			status = "disabled";
		};
	};

};

