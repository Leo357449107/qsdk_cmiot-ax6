/dts-v1/;

#include "EASY80920.dtsi"

/ {
	compatible = "lantiq,easy80920-nor", "lantiq,easy80920", "lantiq,xway", "lantiq,vr9";
	model = "Intel EASY80920 Nor";
};

&localbus {
	nor@0 {
		compatible = "lantiq,nor";
		bank-width = <2>;
		reg = <0 0x0 0x2000000>;
		#address-cells = <1>;
		#size-cells = <1>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "uboot";
				reg = <0x00000 0x10000>;
			};

			partition@10000 {
				label = "uboot_env";
				reg = <0x10000 0x10000>;
			};

			partition@20000 {
				label = "firmware";
				reg = <0x20000 0x7e0000>;
			};
		};
	};
};
